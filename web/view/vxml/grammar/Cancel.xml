<?xml version= "1.0"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" xml:lang="en-US" root="CANCEL">
    <rule id="CANCEL"> 
        <one-of>
            <item>
                <one-of> 
                    <item> 
                        cancel   
                    </item> 
                    <item> 
                        stop   
                    </item> 
                    <item> 
                        go back   
                    </item> 
                    <item> 
                        no   
                    </item> 
                    <item> 
                        stop to cancel 
                    </item>
                    <item> 
                        dtmf-star
                    </item>
                </one-of>
                <tag> <![CDATA[  <cancel "cancel">  ]]>  </tag>
            </item>
            <item>
                <one-of>
                    <item> 
                        connect  
                    </item>
                </one-of> 
                <tag> <![CDATA[  <cancel "connect">  ]]>  </tag> 
            </item>
        </one-of>
    </rule>
</grammar>
